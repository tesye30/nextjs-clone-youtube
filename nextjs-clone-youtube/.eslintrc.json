{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
       
        "react/jsx-filename-extension": [1, {"extensions": [".js", ".jsx" ]}],
        "react/prop-types":"off",
        "react/jsx-props-no-spreading": ["error",{
          "html":"ignore",
          "custom":"ignore",
          "execptions":[""]
        }]
    }
}
